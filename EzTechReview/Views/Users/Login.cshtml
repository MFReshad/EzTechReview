            <div class="registration">

                <div class="signin-form profile">
                    <h2>Sign in Form</h2>
                    <div class="login-form">
                        <form action="Login" method="post">
                            <input type="email" name="UserEmail" placeholder="E-mail" />

                            <input type="password" name="UserPassword" placeholder="Password" />
                            @if (msg != null)
                            {

                <p class="text-danger">@msg</p>}
                            <div class="tp">
                                <input type="submit" value="SIGN IN">
                            </div>
                        </form>
                    </div>
                    <div class="login-social-grids">
                        <ul>
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#"><i class="fa fa-rss"></i></a></li>
                        </ul>
                    </div>
                    <p><a href="@Url.Action("Registration","Users")"> Don't have an account?</a></p>

                    <h6>
                        <p>@Html.ActionLink("Back To Home", "Index", "Home")</p>
                    </h6>
                </div>
            </div>
            <!--copy rights start here-->
            <div class="copyrights_agile">
                <p>© All Rights Reserved by Tech Review Point </p>
            </div>
            <!--copy rights end here-->
